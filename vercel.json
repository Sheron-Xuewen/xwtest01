{
  "headers": [
    {
      "source": "/Build/(.*)",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*).br",
      "headers": { "content-encoding": "br" },
      "dest": "/$1"
    }
  ]
}
